   "cell_type": "code",
   "source": [
    "def factorial(n, memory={}):\n",
    "  if n == 0 or n == 1:\n",
    "    return 1\n",
    "  if n in memory:\n",
    "    return memory[n]\n",
    "  memory[n] = n * factorial(n - 1, memory)\n",
    "  print(memory)\n",
    "  return memory[n]\n",
    "\n",
    "print(factorial(7))\n",
    "\n",
    "print(factorial(5))\n",
    "\n",
    "print(factorial(10))"
   ],
